          <dev:defaultValue>Segoi UI</dev:defaultValue>
        </command:parameter>
        <command:parameter required="false" variableLength="true" globbing="false" pipelineInput="True (ByPropertyName)" position="named" aliases="size">
          <maml:name>FontSize</maml:name>
          <maml:description>
